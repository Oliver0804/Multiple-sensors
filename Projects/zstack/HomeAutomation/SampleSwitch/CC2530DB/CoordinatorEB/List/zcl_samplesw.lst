    826          /*********************************************************************
    827           * @fn      zclSampleSw_ProcessOTAMsgs
    828           *
    829           * @brief   Called to process callbacks from the ZCL OTA.
    830           *
    831           * @param   none
    832           *
    833           * @return  none
    834           */
    835          static void zclSampleSw_ProcessOTAMsgs( zclOTA_CallbackMsg_t* pMsg )
    836          {
    837            uint8 RxOnIdle;
    838          
    839            switch(pMsg->ota_event)
    840            {
    841            case ZCL_OTA_START_CALLBACK:
    842              if (pMsg->hdr.status == ZSuccess)
    843              {
    844                // Speed up the poll rate
    845                RxOnIdle = TRUE;
    846                ZMacSetReq( ZMacRxOnIdle, &RxOnIdle );
    847                NLME_SetPollRate( 2000 );
    848              }
    849              break;
    850          
    851            case ZCL_OTA_DL_COMPLETE_CALLBACK:
    852              if (pMsg->hdr.status == ZSuccess)
    853              {
    854                // Reset the CRC Shadow and reboot.  The bootloader will see the
    855                // CRC shadow has been cleared and switch to the new image
    856                HalOTAInvRC();
    857                SystemReset();
    858              }
    859              else
    860              {
    861                // slow the poll rate back down.
    862                RxOnIdle = FALSE;
    863                ZMacSetReq( ZMacRxOnIdle, &RxOnIdle );
    864                NLME_SetPollRate(DEVICE_POLL_RATE);
    865              }
    866              break;
    867          
    868            default:
    869              break;
    870            }
    871          }
    872          #endif // defined (OTA_CLIENT) && (OTA_CLIENT == TRUE)
